    "1634155024d781abbc": {
      "type": "image-with-text",
      "blocks": {
        "1634155024d781abbc-0": {
          "type": "heading",
          "settings": {
            "heading": "How LifeThreads helps groups achieve their goals"
          }
        },
        "1634155024d781abbc-1": {
          "type": "text",
          "settings": {
            "text": "<p>We believe that every customer deserves a hassle-free program experience. From providing safer scrubs to customized program experiences, we control the process from the factory to your doorstep. Our goal is to support you in achieving your goals!<\/p>"
          }
        }
      },
      "block_order": [
        "1634155024d781abbc-0",
        "1634155024d781abbc-1"
      ],
      "settings": {
